{
	"manifest_version": 2,

	"name": "trip.Pin",
	"description": "This extension allows users to pin trips",
	"version": "1.0",

	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "trip.Pin"
	},
	"background": {
		"scripts": ["popup.js", "scrape.js", "trip.js", "background.js"],
		"persistent": false
	},
	"permissions": [
		"notifications"
	]
}